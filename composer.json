{
    "name": "proklung/bitrix-twig-bundle",
    "description": "Twig bundle for custom Bitrix + Symfony.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Gavrilov Fedy",
            "email": "fedor.gavrilov.75@mail.ru",
            "homepage": "https://github.com/ProklUng"
        }
    ],
    "support": {
        "issues": "https://github.com/ProklUng/bitrix.twig.bundle/issues",
        "source": "https://github.com/ProklUng/bitrix.twig.bundle"
    },
    "autoload": {
        "psr-4": {
            "Prokl\\BitrixTwigBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prokl\\BitrixTwigBundle\\Tests\\": "tests"
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "require": {
        "php": ">=7.3 || ^8.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/config": "^4.4 || ^5.0",
        "twig/twig": "^2.14.6 | ^3"
    },
    "require-dev": {
        "proklung/phpunit-testing-tools": "^1.4",
        "symfony/var-dumper": "^4.0 || ^5.0",
        "icanhazstring/composer-unused": "^0.7.5"
    }
}
